{
  "name": "discord-bot",
  "version": "5.0.0",
  "description": "The discord bot with various commands, created by syntony666.",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx tsc",
    "migrate": "npx -y sequelize-cli db:migrate",
    "migrate:prod": "npx -y sequelize-cli db:migrate --env production",
    "start": "node -r dotenv/config ./dist/index.js",
    "app": "npm run build && npm run start"
  },
  "keywords": [
    "discord-bot"
  ],
  "author": "syntony666",
  "license": "GPL-3.0-only",
  "devDependencies": {
    "@types/node": "^20.14.8",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "@discordeno/bot": "^19.0.0-next.af6ffc3",
    "@discordjs/formatters": "^0.4.0",
    "dotenv": "^16.4.5",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3"
  }
}
